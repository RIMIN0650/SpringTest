<?xml version="1.0" encoding="UTF-8"?>

<!-- dtd : xml 에서 사용될 태그를 정의한 문서 -->

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 
 <mapper namespace="com.rimin.spring.test.database.repository.UsedGoodsRepository">
 	
 	<select id="selectUsedGoodsList" 
 	resultType="com.rimin.spring.test.database.domain.UsedGoods">
 		SELECT 
 			`id`
 			, `sellerId`
 			, `title`
 			, `price`
 			, `description`
 			, `image`
 			, `createdAt`
 			, `updatedAt`
 		FROM 
 			`used_goods`
 		
 	
 	</select>
 
 </mapper>