<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 
 
 <mapper namespace="com.rimin.spring.test.front.repository.BookingRepository">
 	
 	<select id="selectBookList" resultType="com.rimin.spring.test.front.domain.Booking">
 		SELECT 
 			`id`
 			, `name`
 			, `date`
 			, `day`
 			, `headcount`
 			, `phoneNumber`
 			, `state`
 			, `createdAt`
 			, `updatedAt`
 		FROM
 			`booking`
 			
 		
 	
 	
 	
 	
 	
 	</select>
 
 
 
 </mapper>